<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" stylesheets="@css/host.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button layoutX="-30.0" layoutY="280.0" mnemonicParsing="false" onAction="#backBtnAction" opacity="0.0" prefHeight="161.0" prefWidth="164.0" style="-fx-background-radius: 40;" text="BACK" />
      <Button layoutX="1161.0" layoutY="286.0" mnemonicParsing="false" onAction="#forwardBtnAction" opacity="0.0" prefHeight="161.0" prefWidth="142.0" style="-fx-background-radius: 40;" text="FORWARD" />
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#helpBtnAction" opacity="0.0" prefHeight="70.0" prefWidth="69.0" style="-fx-background-radius: 100;" text="HELP" />
      <Label fx:id="hostLabel" layoutX="112.0" layoutY="151.0" />
      <TextField layoutX="343.0" layoutY="432.0" opacity="0.65" prefHeight="123.0" prefWidth="618.0" promptText="Enter user name here" style="-fx-background-color: #000000; -fx-prompt-text-fill: white; -fx-font-size: 45; -fx-text-fill: white;" />
      <ToggleButton layoutX="812.0" layoutY="217.0" mnemonicParsing="false" prefHeight="52.0" prefWidth="42.0" selected="true" text="On">
         <toggleGroup>
            <ToggleGroup fx:id="On" />
         </toggleGroup>
      </ToggleButton>
      <ToggleButton layoutX="854.0" layoutY="217.0" mnemonicParsing="false" prefHeight="52.0" prefWidth="42.0" text="Off" toggleGroup="$On" />
   </children>
</AnchorPane>
